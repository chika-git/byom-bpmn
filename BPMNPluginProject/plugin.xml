<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.bpmn2.modeler.runtime">
      <runtime
            class="bpmn2.tutorial.ImixsRuntimeExtension"
            id="org.imixs.workflow.bpmn.runtime"
            name="Imixs-Workflow">
      </runtime>
      <customTask
            category="Imixs-Workflow"
            featureContainer="bpmn2.tutorial.TestTaskElementFeatureContainer1"
            id="org.imixs.workflow.bpmn.customTask"
            name="AbstractActivity"
            runtimeId="org.imixs.workflow.bpmn.runtime"
            type="ServiceTask">
         <property
               name="type"
               value="TestTask">
         </property>
      </customTask>
      <model
            resourceFactory="model.util.ModelResourceFactoryImpl"
            runtimeId="org.imixs.workflow.bpmn.runtime"
            uri="http://www.cs.manchester.ac.uk/bpmnext">
      </model>
      <propertyTab
            class="bpmn2.tutorial.AbstractBpmn2PropertySection"
            features="extensionValues#AbstractActivity.context"
            id="org.imixs.workflow.bpmn.propertyid"
            indented="true"
            label="Abstract Activity"
            runtimeId="org.imixs.workflow.bpmn.runtime"
            type="org.eclipse.bpmn2.ServiceTask">
      </propertyTab>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated byom -->
      <package
            uri="http://www.cs.manchester.ac.uk/bpmnext"
            class="model.ModelPackage"
            genModel="model/byom.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <!-- @generated byom -->
      <parser
            type="model"
            class="model.util.ModelResourceFactoryImpl"/>
   </extension>
</plugin>
